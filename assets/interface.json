{
    "name": "MaHR",
    "version": "0.1",
    "url": "https://github.com/mxia9416/MACC",
    "default_controller": "adb",
    "lock_controller": true,
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "国服",
            "path": [
                "{PROJECT_DIR}/resource/base"
            ]
        },
        {
            "name": "台服",
            "path": [
                "{PROJECT_DIR}/resource/base",
                "{PROJECT_DIR}/resource/taiwan"
            ]
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "{PROJECT_DIR}/agent/main.py"
        ]
    },
    "task": [
        {
            "name": "进入游戏",
            "entry": "启动游戏"
        }
    ],
    "option": {
        "任务选项1": {
            "cases": [
                {
                    "name": "文本1",
                    "pipeline_override": {
                        "OcrTask": {
                            "text": "文本1"
                        }
                    }
                },
                {
                    "name": "文本2",
                    "pipeline_override": {
                        "OcrTask": {
                            "text": "文本2"
                        }
                    }
                }
            ]
        }
    }
}
